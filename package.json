{
  "name": "simbpmn",
  "productName": "SimBPMN Visualizer",
  "version": "1.0.202210091",
  "description": "Specification application for simbpmn notation",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "debug": "electron-forge start --inspect-electron",
    "test": "jasmine"
  },
  "keywords": [],
  "author": {
    "name": "Sven HÃ¶pfner, Michel Mirsayyah",
    "url": "https://www.ost.ch"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "extraResource": "src/translations/electron"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "simbpmn"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@types/jquery": "^3.5.14",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "^6.0.0",
    "electron": "^19.0.4",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@bpmn-io/properties-panel": "^0.11.0",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@types/adm-zip": "^0.5.0",
    "@types/electron-prompt": "^1.6.1",
    "adm-zip": "^0.5.9",
    "babel-loader": "^8.2.4",
    "bpmn-js": "^9.0.3",
    "bpmn-js-color-picker": "^0.4.0",
    "bpmn-js-properties-panel": "^1.0.0-alpha.5",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^10.2.4",
    "diagram-js-minimap": "^2.1.1",
    "electron-prompt": "^1.7.0",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "i18next": "^21.6.16",
    "i18next-node-fs-backend": "^2.1.3",
    "jasmine": "^4.0.2",
    "jquery": "^3.6.0",
    "linq": "^4.0.0",
    "min-dash": "^3.7.0",
    "path": "^0.12.7",
    "raw-loader": "^4.0.2",
    "walkdir": "^0.4.1"
  }
}
